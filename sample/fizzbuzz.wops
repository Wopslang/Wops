// 
// 2021, Wops Team
//

int n = 100 

for i in 1~n+1~1 $
    out(tostring(i)+": ")
    if i % 15 == 0 ?
        out("fizzbuzz")
    ; ?
        if i % 3 == 0 ?
            out("fizz")
        ; i % 5 == 0 ?
            out("buzz")
        ; ?
            out("none\n")
            continue
        ;
    ;
    out("\n")
;